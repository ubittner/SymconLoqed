    {
      "type": "CheckBox",
      "name": "UseActivityLog",
      "caption": "Use activity log"
    },
    {
      "type": "NumberSpinner",
      "name": "ActivityLogMaximumEntries",
      "caption": "Number of maximum activity entries",
      "minimum": 1,
      "maximum": 50,
      "suffix": "entries"
    }
  ],
  "actions": [
    {
      "type": "ExpansionPanel",
      "caption": "Developer area",
      "items": [
        {
          "type": "Label",
          "caption": "State"
        },
        {
          "type": "Button",
          "caption": "Update Device State",
          "onClick": "LOQED_UpdateDeviceState($id);"
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "Label",
          "caption": "Webhooks"
        },
        {
          "type": "Button",
          "caption": "List",
          "onClick": "$data=LOQED_ListBridgeWebhooks($id); print_r(json_decode($data, true));"
        },
        {
          "type": "RowLayout",
          "items": [
            {
              "type": "NumberSpinner",
              "name": "WebhookID",
              "caption": "Webhook ID"
            },
            {
              "type": "Label",
              "caption": " "
            },
            {
              "type": "PopupButton",
              "caption": "Delete",
              "popup": {
                "caption": "Do you really want to delete the webhook?",
                "items": [
                  {
                    "type": "Button",
                    "caption": "Delete",
                    "onClick": "$data=LOQED_DeleteBridgeWebhook($id, $WebhookID); print_r(json_decode($data, true));"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "Label",
          "caption": " "
        },
        {
          "type": "Label",
          "caption": "Switching"
        },
        {
          "type": "TestCenter"
        }
      ]
    }
  ],
